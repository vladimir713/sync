<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" prefHeight="454.0" prefWidth="767.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sync.FrontController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="source1Button" mnemonicParsing="false" onAction="#onOpenFileButton1" prefHeight="25.0" prefWidth="727.0" text="Выбрать источник 1" />
    <TextArea fx:id="path1" />
    <Button fx:id="source2Button" mnemonicParsing="false" onAction="#onOpenFileButton2" prefHeight="25.0" prefWidth="727.0" text="Выбрать источник 2" />
    <TextArea fx:id="path2" />
    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Сравнить" />
</VBox>
